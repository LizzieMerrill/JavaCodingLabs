class Circle {
  public static void main(String[] args) {
     
     double radius;
     double diameter;
     double circumference;
     double area;
     double semicircleArea;
     
     radius = 10.25;
     diameter = 2*radius;
     circumference = 2*3.1415*radius;
     area = 3.1415*radius*radius;
     semicircleArea = 0.5*3.1415*radius*radius;
     
      System.out.println("Properties of a Circle");
      System.out.println("Radius             : " + radius);
      System.out.println("Diameter           : " + diameter);
      System.out.println("Circumference      : " + circumference);
      System.out.println("Area               : " + area);
      System.out.println("Area of Semicircle : " + semicircleArea);
      System.out.println("");
      System.out.println("Properties \"Rounded\" Down");
      System.out.println("Radius             : " + (int)radius);
      System.out.println("Diameter           : " + (int)diameter);
      System.out.println("Circumference      : " + (int)circumference);
      System.out.println("Area               : " + (int)area);
      System.out.println("Area of Semicircle : " + (int)semicircleArea);


     
  }
}
