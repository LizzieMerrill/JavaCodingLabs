import java.util.Scanner;

public class Converter {
   public static void main(String[] args) {
      Scanner scnr = new Scanner(System.in);
      
      //This program takes distance in feet and inches and converts it to meters, rounding up to the second decimal place.
      
      System.out.print("Enter Feet   : ");
      int feet = scnr.nextInt();
      System.out.print("\nEnter Inches : ");
      double inches = scnr.nextDouble();
      
      double feetToInches = feet * 12.0;
      
      double meters = (feetToInches + inches) / 39.37;
      
      System.out.printf("%d' and %.2f\" is %.2f Meters.", feet, inches, meters);
      System.out.print("\n");

      
   }
}
