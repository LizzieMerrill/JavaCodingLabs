import java.util.Scanner;

public class Paint {
   public static void main(String[] args) {
      Scanner scnr = new Scanner(System.in);
      
      int wallHeight;
      int wallWidth;
      int wallArea;
      
      System.out.print("Enter wall height (feet) : ");
      wallHeight = scnr.nextInt();
      
      System.out.print("\nEnter wall width (feet) : ");
      wallWidth = scnr.nextInt();
      
      wallArea = wallHeight * wallWidth;
      
      double painT;
      //Gallons of paint needed.
      painT = (double)wallArea / 350.0;
      //One gallon of paint will cover 350 square feet.
      
      double cansOfPain;
      //Cans of paint needed. Each can is 1 gallon.
      cansOfPain = Math.ceil(painT);
      
      System.out.println("");
      System.out.println("");
      System.out.printf("Wall area : " + wallArea + " square feet");

      System.out.printf("\nPaint needed : %.2f gallons\n", painT);
     
      System.out.printf("Cans needed : " + (int)cansOfPain + " can(s)\n");
      
      
   }
}
